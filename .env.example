# AI-Army Configuration
# Copy this file to .env and fill in your values

# Anthropic API key for Claude (required for all agents)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# GitHub Personal Access Token (repo, issues, pull_requests scopes)
GITHUB_TOKEN=your_github_token_here

# Target repository in owner/repo format (e.g. myorg/my-app)
GITHUB_TARGET_REPO=owner/repo-name

# Optional: OpenAI API key for fallback tools
# OPENAI_API_KEY=your_openai_key_here
